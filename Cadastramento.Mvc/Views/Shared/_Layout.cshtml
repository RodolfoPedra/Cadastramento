                                </a>
                            </li>
                            @{
                                var permissoesUsuario = new BaseService<usuariomenuperfilpermissao>().ObterVarios(x => x.perfilid == SessaoUsuario.Perfil.perfilid).Select(x => x.usuariomenu).OrderBy(x => x.ordem).ToList();
                                var permissoesId = permissoesUsuario.Select(x => x.usuariomenuid).ToList();
                                foreach (var item in permissoesUsuario)
                                {
                                    if (item.paiusuariomenuid == null && !item.usuariomenu1.Any() && !string.IsNullOrEmpty(item.url))
                                    {
                                        <li class="kt-menu__item" aria-haspopup="true">
                                            <a href="@Url.Content("~" + item.url)" class="kt-menu__link">
                                                <i class="kt-menu__link-icon @item.classeicone"></i>
                                                <span class="kt-menu__link-text">@item.texto</span>
                                            </a>
                                        </li>
                                    }
                                    else if (item.paiusuariomenuid == null && item.usuariomenu1.Any())
                                    {
                                        var filhos = item.usuariomenu1.Where(x => permissoesId.Contains(x.usuariomenuid)).OrderBy(x => x.ordem).ToList();

                                        <li class="kt-menu__item kt-menu__item--submenu" aria-haspopup="true" data-ktmenu-submenu-toggle="hover">
                                            <a href="javascript:;" class="kt-menu__link kt-menu__toggle"><i class="kt-menu__link-icon @item.classeicone"></i><span class="kt-menu__link-text">@item.texto</span><i class="kt-menu__ver-arrow la la-angle-right"></i></a>
                                            <div class="kt-menu__submenu " kt-hidden-height="80" style="display: none; overflow: hidden;">
                                                <span class="kt-menu__arrow"></span>
                                                <ul class="kt-menu__subnav">
                                                    <li class="kt-menu__item  kt-menu__item--parent" aria-haspopup="true"><span class="kt-menu__link"><span class="kt-menu__link-text">@item.texto</span></span></li>
                                                    @foreach (var item2 in filhos)
                                                    {
                                                        <li class="kt-menu__item " aria-haspopup="true"><a href="@Url.Content("~" + item2.url)" class="kt-menu__link "><i class="kt-menu__link-bullet kt-menu__link-bullet--dot"><span></span></i><span class="kt-menu__link-text">@item2.texto</span></a></li>
