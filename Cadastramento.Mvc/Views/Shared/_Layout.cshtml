    <script>
    var content = "@Url.Content("~/")";

    $.extend(true, $.fn.dataTable.defaults, {
        language: {
            url: content  + "assets/plugins/custom/datatables/datatables_pt-br.json"
        }
    } );

    //Configurações globais do plugin blockUI
    $.blockUI.defaults.message = `<h3><img src='${content}Content/img/loader1.gif' /></h3>`;
    $.blockUI.defaults.baseZ = "2000";
    $.blockUI.defaults.css.cursor = "default";
    $.blockUI.defaults.css.border = "0px solid";
    $.blockUI.defaults.css.backgroundColor = "transparent";
    $.blockUI.defaults.overlayCSS.backgroundColor = "#E6E6E6";
    $.blockUI.defaults.overlayCSS.cursor = "default";

    //Bloqueando/Desbloqueando a página automaticamente nas requisições ajax
    $(document).ajaxStart($.blockUI).ajaxStop($.unblockUI);

    $(document).ready(function() {
        $("select").not(".swal2-select").select2({
            language: 'pt-BR',
            placeholder: "SELECIONE...",
            allowClear: true,
            width: '100%'
        });

        //Bloqueando a página automaticamente ao dar submit em um form
        $("form").each(function () {
            $(this).on("submit", function () {
                $.blockUI();
            });
        });
    });

    var KTAppOptions = {
        "colors": {
            "state": {
                "brand": "#2c77f4",
                "light": "#ffffff",
                "dark": "#282a3c",
                "primary": "#5867dd",
                "success": "#34bfa3",
                "info": "#36a3f7",
                "warning": "#ffb822",
                "danger": "#fd3995"
            },
            "base": {
                "label": ["#c5cbe3", "#a1a8c3", "#3d4465", "#3e4466"],
                "shape": ["#f0f3ff", "#d9dffa", "#afb4d4", "#646c9a"]
            }
        }
    };

        function AbrirModalAlterarSenha_Click() {
            //Recarrega o body
            $.get(content + "Account/ModalAlterarSenha",
                null,
                (data) => {
                    $("#modalMedia .modal-title").text("Alterar Senha");
                    $("#modalMedia .modal-body").html(data);
                });

            //Abre a modal
            $("#modalMedia").modal('show');
        }
    </script>

